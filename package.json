{
  "name": "supplier",
  "version": "2.0.4-SNAPSHOT",
  "description": "Supplier Microservice. Provides base CRUD Rest API for basic Supplier data manipulation.",
  "scripts": {
    "start": "npm run build ; node src/server/index.js",
    "dev": "npm run setup-consul ; ${NODE_PATH}/.bin/nodemon --watch src/server src/server/index.js",
    "build": "npm run build:client",
    "build:client": "${NODE_PATH}/.bin/webpack --config ./webpack.production.config.js --progress --profile --colors --bail",
    "test": "npm run clean ; npm run setup-consul ; ${NODE_PATH}/.bin/nyc ${NODE_PATH}/.bin/mocha -R mocha-junit-reporter src/**/*.test.js",
    "clean": "${NODE_PATH}/.bin/rimraf esdoc testdb.sqlite .nyc_output coverage test-results.xml",
    "setupConsul": "curl -X PUT -d 'bnp' http://${HOST_IP}:8500/v1/kv/supplier/db-init/database ; curl -X PUT -d 'root' http://${HOST_IP}:8500/v1/kv/supplier/db-init/user ; curl -X PUT -d 'notSecurePa55w0rd' http://${HOST_IP}:8500/v1/kv/supplier/db-init/password",
    "lint": "${NODE_PATH}/.bin/eslint src",
    "esdoc": "${NODE_PATH}/.bin/rimraf esdoc && esdoc -c esdoc.json",
    "domain-doc": "mkdir -p wiki/domain-doc ; ${NODE_PATH}/.bin/sq2md --path-bl=index.js -o file-per-src -p wiki/domain-doc --field-bl=createdAt,updatedAt --output-ext=.model.md src/server/db/models",
    "doc": "npm run domain-doc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OpusCapita/supplier"
  },
  "author": {
    "name": "Andrei Mourzenkov",
    "email": "andrei.mourzenkov@jcatalog.com"
  },
  "license": "jCatalog",
  "readme": "README.md",
  "dependencies": {
    "babel-loader": "6.4.1",
    "babel-runtime": "6.23.0",
    "bluebird": "3.5.0",
    "classnames": "2.2.5",
    "css-loader": "0.28.0",
    "epilogue": "0.7.1",
    "intel": "1.1.2",
    "jquery": "3.2.1",
    "json-loader": "0.5.4",
    "less": "2.7.2",
    "less-loader": "4.0.3",
    "lodash": "4.17.4",
    "ocbesbn-config": "^2.2.0",
    "ocbesbn-db-init": "^1.1.0",
    "ocbesbn-web-init": "^1.0.1",
    "opuscapita-i18n": "1.0.10",
    "network": "0.3.2",
    "react": "15.4.2",
    "react-addons-test-utils": "15.4.2",
    "react-bootstrap": "0.30.8",
    "react-dom": "15.4.2",
    "react-router": "3.0.0",
    "react-select": "1.0.0-rc.3",
    "react-spinkit": "1.1.11",
    "rimraf": "2.6.1",
    "sequelize": "3.30.4",
    "sequelize-to-markdown": "1.1.11",
    "style-loader": "0.16.1",
    "superagent": "3.5.2",
    "superagent-bluebird-promise": "4.2.0",
    "underscore": "1.8.3",
    "validate.js": "0.11.1",
    "webpack": "2.3.2"
  },
  "devDependencies": {
    "babel-cli": "6.24.0",
    "babel-core": "6.24.0",
    "babel-eslint": "7.2.1",
    "babel-plugin-istanbul": "4.1.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.0",
    "babel-preset-react": "6.23.0",
    "babel-preset-stage-0": "6.22.0",
    "babel-register": "6.24.0",
    "chai": "3.5.0",
    "esdoc": "0.5.2",
    "esdoc-es7-plugin": "0.0.3",
    "eslint": "3.18.0",
    "eslint-config-opuscapita": "1.0.4",
    "eslint-plugin-react": "6.10.3",
    "mocha": "3.2.0",
    "mocha-junit-reporter": "1.13.0",
    "nodemon": "1.11.0",
    "nyc": "10.2.0"
  }
}
