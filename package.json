{
  "name": "supplier",
  "version": "2.0.4-SNAPSHOT",
  "description": "Supplier Microservice. Provides base CRUD Rest API for basic Supplier data manipulation.",
  "scripts": {
    "start": "node src/server/index.js",
    "dev": "npm run setup-consul ; nodemon --watch src/server src/server/index.js",
    "local": "npm run setup-consul ; NODE_ENV=development nodemon --watch src/server www/index.js",
    "dev:production": "npm run setup-consul ; npm start",
    "build": "npm run build:client",
    "build:client": "cross-env NODE_ENV=production ; webpack -p --config ./webpack.production.config.js --profile --colors --bail",
    "test": "npm run clean ; npm run setup-consul ; nyc mocha -R mocha-junit-reporter src/**/*.test.js",
    "clean": "rimraf esdoc testdb.sqlite .nyc_output coverage test-results.xml",
    "setup-consul": "curl -X PUT -d ${MYSQL_DATABASE} http://${HOST_IP}:8500/v1/kv/supplier/db-init/database ; curl -X PUT -d 'root' http://${HOST_IP}:8500/v1/kv/supplier/db-init/user ; curl -X PUT -d ${MYSQL_ROOT_PASSWORD} http://${HOST_IP}:8500/v1/kv/supplier/db-init/password ; curl -X PUT -d 'true' http://${HOST_IP}:8500/v1/kv/supplier/db-init/populate-test-data ; curl -X PUT -d ${REDIS_AUTH} http://${HOST_IP}:8500/v1/kv/supplier/redis/password",
    "lint": "eslint src",
    "esdoc": "rimraf esdoc && esdoc -c esdoc.json",
    "domain-doc": "mkdir -p wiki/domain-doc ; sq2md --path-bl=index.js -o file-per-src -p wiki/domain-doc --field-bl=createdAt,updatedAt --output-ext=.model.md src/server/db/models",
    "rest-doc": "mkdir -p wiki/rest-doc ; ./node_modules/.bin/raml2md -o file-per-resource -p wiki/rest-doc --output-ext=.endpoint.md rest-doc/main.raml",
    "doc": "npm run domain-doc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OpusCapita/supplier"
  },
  "author": {
    "name": "Andrei Mourzenkov",
    "email": "andrei.mourzenkov@jcatalog.com"
  },
  "license": "jCatalog",
  "readme": "README.md",
  "dependencies": {
    "babel-loader": "7.0.0",
    "babel-runtime": "6.23.0",
    "bluebird": "3.5.0",
    "classnames": "2.2.5",
    "cross-env": "4.0.0",
    "css-loader": "0.28.1",
    "intel": "1.1.2",
    "json-loader": "0.5.4",
    "less": "2.7.2",
    "less-loader": "4.0.3",
    "lodash": "4.17.4",
    "ocbesbn-config": "2.2.6",
    "ocbesbn-db-init": "1.1.9",
    "ocbesbn-redis-events": "^1.0.3",
    "ocbesbn-service-client": "^1.0.1",
    "ocbesbn-web-init": "2.1.1",
    "opuscapita-i18n": "github:patrykkopycinski/js-i18n",
    "opuscapita-react-dates": "0.0.1-beta.13",
    "@opuscapita/react-loaders": "0.1.5",
    "react": "15.4.2",
    "react-bootstrap": "0.31.0",
    "react-dom": "15.4.2",
    "react-select": "1.0.0-rc.4",
    "sequelize": "3.30.4",
    "style-loader": "0.17.0",
    "superagent": "3.5.2",
    "superagent-bluebird-promise": "4.2.0",
    "underscore": "1.8.3",
    "validate.js": "0.11.1",
    "scriptjs": "2.5.8"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-plugin-istanbul": "4.1.3",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "6.24.1",
    "chai": "3.5.0",
    "esdoc": "0.5.2",
    "esdoc-es7-plugin": "0.0.3",
    "eslint": "3.19.0",
    "eslint-config-opuscapita": "1.0.4",
    "eslint-plugin-react": "7.0.1",
    "mocha": "3.4.1",
    "mocha-junit-reporter": "1.13.0",
    "nodemon": "1.11.0",
    "nyc": "10.3.2",
    "raml-to-markdown": "1.1.3",
    "raml2html": "3.0.1",
    "react-addons-test-utils": "15.4.2",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "rimraf": "2.6.1",
    "sequelize-to-markdown": "1.1.13",
    "webpack": "2.5.1"
  }
}
